 @model IEnumerable<DispatchAgency>

<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">DispatchAgency</h4>

            <div class="table-responsive pt-3">
                <a class="btn btn-success" asp-action="Create"> Create </a>
                <hr />
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Agency Name</th>
                            <th>Address</th>
                            <th>EmailId</th>
                            <th>MobileNo</th>
                            <th>Contact Person Name </th>

                        </tr>
                    </thead>
                    <tbody>
                        @{
                            int srno = 1;
                        }
                        @foreach (var temp in Model)
                        {
                            <tr>
                                <td>@srno</td>
                                <td>@temp.AgencyName</td>
                                <td>@temp.Address</td>
                                <td>@temp.Emial</td>
                                <td>@temp.MobileNo</td>
                                <td>@temp.ContactPerson</td>
                                <td>
                                    <a class="btn btn-outline-primary" asp-action="Edit" asp-route-id="@temp.DispatchAgencyId">Edit</a>
                                    |
                                    <a class="btn btn-outline-primary" asp-action="Delete" asp-route-id="@temp.DispatchAgencyId">Delete</a>
                                </td>
                            </tr>
                            srno++;
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>