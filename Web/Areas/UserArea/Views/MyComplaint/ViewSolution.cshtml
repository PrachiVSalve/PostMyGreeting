  @model Solution
<div class="profile-overview">
    <h5 class="card-title">Solution Details</h5>
    <div class="row">
        <div class="col-lg-6 col-md-6 label ">Solution For Complaint:</div>
        <div class="col-lg-6 col-md-6">@Model.ComplaintId</div>
    </div>

    <div class="row">
        <div class="col-lg-6 col-md-4 label ">Solution Date:</div>
        <div class="col-lg-6 col-md-8">@Model.SolutionDate.ToString("dd-MM-yyyy")</div>
    </div>

    <div class="row">
        <div class="col-lg-6 col-md-4 label">Solution Desc:</div>
        <div class="col-lg-6 col-md-8">@Model.SolutionDescription</div>
    </div>

    <div>
        <a class="btn btn-primary text-center mt-5" asp-action="Index"> Back </a>
    </div>
</div>